.wrapper

  -#= current_user.id
  = bootstrap_form_for(@expense) do |f|
    = f.hidden_field :user_id, {:value => current_user.id}
    = f.text_field :amount, :value => (number_with_precision(f.object.amount, :precision => 2) || "") 
    = f.text_field :category
    = f.text_area :description
    .actions
      = f.submit
